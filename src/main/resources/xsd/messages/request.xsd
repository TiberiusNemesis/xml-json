<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:client="http://www.marlo.com.au/training/client"
           xmlns:request="http://www.marlo.com.au/training/request"
           targetNamespace="http://www.marlo.com.au/training/request">
    <xs:import namespace="http://www.marlo.com.au/training/client" schemaLocation="../store/client.xsd" />
    <xs:element name="request" type="request:request"/>
        <xs:complexType name="request">
            <xs:sequence>
                <xs:element ref="client:client" minOccurs="1" maxOccurs="1"/>
            </xs:sequence>
        </xs:complexType>
</xs:schema>